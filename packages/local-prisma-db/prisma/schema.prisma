generator client {
  provider = "prisma-client-js"
  output   = "./generated"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev(local).db"
}

model User {
  id         Int      @id @unique
  birthDay   DateTime
  name       String?
  cardNumber String   @unique
  isAllowed  Boolean  @default(false)
  location   String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
